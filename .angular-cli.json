{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "SCM App consumers"
  },
  "apps": [
    {
      "root": "src",
      "baseHref": "/",
      "osx": {
        "outputDirectory": "dist/mac"
      },
      "win": {
        "outputDirectory": "dist/win"
      },
      "assets": [
        "assets",
        "favicon.ico",
        "langs"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "../tsconfig.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "./assets/css/materialize.scss",
        "../node_modules/@swimlane/ngx-datatable/src/themes/material.scss",
        "./assets/css/main.scss",
        "./assets/css/tables.scss",
        "./assets/css/default-theme.scss"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/materialize-css/dist/js/materialize.js",
        "./assets/js/autobahn.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  },
  "warnings": {
    "typescriptMismatch": false
  }
}
